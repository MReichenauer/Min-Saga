{
    "version": 2,
    "functions": {
      "api/index.ts": {
        "maxDuration": 120
      }
    },
    "builds": [
      {
        "src": "api/index.ts",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/api/index.ts"
      }
    ]
  }
  